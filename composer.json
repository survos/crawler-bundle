{
    "name": "survos/crawler-bundle",
    "description": "Provides a way to create tests that crawl a site",
    "keywords": [
        "dev",
        "symfony",
        "crawler",
        "testiong"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/stopwatch": "^7.3",
        "symfony/string": "^7.3",
        "symfony/console": "^7.3",
        "symfony/security-bundle": "^7.3",
        "symfony/http-client-contracts": "^3.5",
        "doctrine/orm": "^2.12 || ^3.3",
        "symfony/routing": "^7.3",
        "zenstruck/browser": "^1.9",
        "nette/php-generator": "^4.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "Survos\\CrawlerBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
